<h1>Signat&aacute;rios</h1>
<p id="notice"><%= notice %></p>

<table class="table">
  <thead>
    <tr>
      <th>Nome</th>
      <th>Email</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @contacts.each do |contact| %>
      <tr>
        <td><%= contact.name %></td>
        <td><%= contact.email %></td>
        <td><%= link_to 'Detalhes', contact %></td>
        <td><%= link_to 'Atualizar', edit_contact_path(contact) %></td>
        <td><%= link_to 'Excluir', contact, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
  <tfoot>
    <tr>
      <td colspan="5" class="text-center"><%= link_to 'Novo SignatÃ¡rio', new_contact_path %></td>
    </tr>
  </tfoot>
</table>

